:rdsf:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#appearsIn
:appearsIn rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ;
           rdfs:domain :Character ;
           rdfs:range :Seasons ;
           rdfs:comment "Indicates on which seasons the character appears. Domain: Character. Range: Season. Asymmetric." .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#hasRelationshipWith
:hasRelationshipWith rdf:type owl:ObjectProperty ,
                              owl:SymmetricProperty ;
                     rdfs:domain :Character ;
                     rdfs:range :Character ;
                     rdfs:comment "Indicates with which other characters he or she has mantained a relationship in the show. Domain: Character. Range: Character. Symmetric." .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#interpretedBy
:interpretedBy rdf:type owl:ObjectProperty ,
                        owl:AsymmetricProperty ;
               rdfs:domain :Character ;
               rdfs:range :Actor ;
               rdfs:comment "Indicates the actor that plays the chracter. Domain: Character. Range: Actor. Asymmetric." .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#worksAt
:worksAt rdf:type owl:ObjectProperty ,
                  owl:AsymmetricProperty ;
         rdfs:domain :Character ;
         rdfs:range :Workplace ;
         rdfs:comment "Indicates the workplace of the character. Domain: Character. Range: Workplace. Asymmetric." .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#worksIn
:worksIn rdf:type owl:ObjectProperty ,
                  owl:AsymmetricProperty ;
         rdfs:domain :Character ;
         rdfs:range :Occupation ;
         rdfs:comment "Indicates the occupation of the character. Domain: Character. Range: Occupation. Asymmetric" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#hasGender
:hasGender rdf:type owl:DatatypeProperty ;
           rdfs:domain :Character ;
           rdfs:range xsd:string ;
           rdfs:comment "Indicates the gender of the character. Domain: Character. Range: String." .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#hasLovers
:hasLovers rdf:type owl:DatatypeProperty ;
           rdfs:domain :Character ;
           rdfs:range xsd:integer ;
           rdfs:comment "Indicates how amany relationships the character has had. Domain: Character. Range: Integer" .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Character ;
         rdfs:range xsd:string ;
         rdfs:comment "Indicates the name of different classes. Domain: Character, Actor, Occupation, Workplace, Seasons. Range: String." .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#hasWorked
:hasWorked rdf:type owl:DatatypeProperty ;
           rdfs:domain :Character ;
           rdfs:range xsd:integer ;
           rdfs:comment "Indicates how many workplaces the character has worked in. Domain: Character. Range: Integer" .


###  http://www.semanticweb.org/pablo-acer/ontologies/2022/3/untitled-ontology-4#numberSeasons
:numberSeasons rdf:type owl:DatatypeProperty ;
               rdfs:domain :Character ;
               rdfs:range xsd:integer ;
               rdfs:comment "Indicates how many different seasons the character appears in. Domain: Character. Range: String." .


